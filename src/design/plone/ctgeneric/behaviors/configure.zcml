<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="design.plone.contenttypes"
    >

  <include package="plone.behavior" file="meta.zcml" />

  <plone:behavior
      name="design.plone.contenttypes.behavior.documento_v2"
      title="Documento V2"
      description="Documento V2"
      factory=".documento.DocumentoV2"
      provides=".documento.IDocumentoV2"
      for="*"
      marker=".documento.IDocumentoV2"
      />

  <plone:behavior
      name="design.plone.contenttypes.behavior.argomenti_documento_v2"
      title="Argomenti Documento"
      description="Tassonomia argomenti V2"
      factory=".argomenti.ArgomentiDocumentoV2"
      provides=".argomenti.IArgomentiDocumentoV2"
      for="*"
      marker=".argomenti.IArgomentiDocumentoV2"
      />

  <plone:behavior
      name="design.plone.contenttypes.behavior.address_uo"
      title="Address UO"
