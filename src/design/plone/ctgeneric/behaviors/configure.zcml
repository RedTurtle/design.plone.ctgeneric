      for="*"
      marker=".news_additional_fields.INewsAdditionalFieldsV2"
      />

      <plone:behavior
      name="design.plone.contenttypes.behavior.contatti_uo_v2"
      title="Contatti V2"
      description="Behavior contatti per UO."
      factory=".contatti.ContattiUnitaOrganizzativaV2"
      provides=".contatti.IContattiUnitaOrganizzativaV2"
