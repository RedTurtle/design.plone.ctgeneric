      provides=".evento.IEventoV2"
      for="*"
      marker=".evento.IEventoV2"
      />
    
    <plone:behavior
      name="design.plone.contenttypes.behavior.luogo_v2"
      title="Metadati luogo V2"
      description=""
      factory=".luogo.LuogoV2"
      provides=".luogo.ILuogoV2"
      for="*"
      marker=".luogo.ILuogoV2"
      />

